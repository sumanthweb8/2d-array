#include <stdio.h>

int main() 
{
    int m,n,mat[100][100],row,col;
    
    printf("enter row & col valuer\n ");
    scanf("%d%d",&m,&n);
    printf("enter the elements of an array \n");
    
    for(row=0;row<m;row++)
    {
        for(col=0;col<n;col++)
        {
            scanf("%d",&mat[row][col]);
        }
    }
    
    printf("array element are :\n");
    
    for(row=0;row<m;row++)
    {
        for (col=0;col<n;col++)
        {
            printf("%d",mat[row][col]);
        }
        
    }
}    
    
    
